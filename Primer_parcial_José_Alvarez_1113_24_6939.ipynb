        "#Conjetura de Collatz\n",
        "n = int(input(\"Ingrese un número entero: \"))\n",
        "\n",
        "while n>1:\n",
        "  if n%2==0:\n",
        "    n= n/2\n",
        "    print(n)\n",
        "  else:\n",
        "    n= n*3+1\n",
        "    print(n)\n",
        "while n<1:\n",
        "  print(\"No cumples las condiciones\")\n",
        "  break"
      ],
